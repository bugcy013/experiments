{
  "bootstrap":{
    "udpserver":{
      "module":"wishbone.iomodules.udpserver",
      "class":"UDPServer",
      "variables":{
        "port":8649
      }
    },
    "stdout":{
      "module":"wishbone.modules.stdout",
      "class":"STDOUT",
      "variables":{
        "purge":true
      }
    }
  },
  "routingtable":{
    "udpserver.inbox":[
      "stdout.inbox"
    ]
  }
}
