{
  "bootstrap": {
    "broker": {
      "group": "wishbone.iomodule", 
      "module": "Broker", 
      "variables": {
        "consume_queue": "wishbone", 
        "delivery_mode": 1, 
        "host": "sandbox", 
        "no_ack": true, 
        "password": "guest", 
        "prefetch_count": 200, 
        "username": "guest", 
        "vhost": "/"
      }
    }, 
    "dump": {
      "group": "wishbone.iomodule", 
      "module": "DumpData", 
      "variables": {
        "amount": 5000
      }
    }, 
    "header": {
      "group": "wishbone.module", 
      "module": "BrokerHeader", 
      "variables": {
        "exchange": "", 
        "key": "wishbone"
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 1, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "broker.inbox": [
      "header.inbox"
    ], 
    "dump.inbox": [
      "header.inbox"
    ], 
    "header.outbox": [
      "broker.outbox"
    ]
  }
}