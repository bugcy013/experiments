{
  "bootstrap": {
    "namedpipe": {
      "group": "wishbone.iomodule", 
      "module": "NamedPipeServer", 
      "variables": {
        "path": "/tmp/namedpipe"
      }
    }, 
    "tcpclient": {
      "group": "wishbone.iomodule", 
      "module": "TCPClient", 
      "variables": {
        "pool": [
          "10.1.63.175:2003"
        ]
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "namedpipe.inbox": [
      "tcpclient.inbox"
    ]
  }
}