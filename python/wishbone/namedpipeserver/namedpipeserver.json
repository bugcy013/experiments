{
  "bootstrap": {
    "namedpipeserver": {
      "group": "wishbone.iomodule", 
      "module": "NamedPipeServer", 
      "variables": {
        "path": "/tmp/namedpipeserver"
      }
    }, 
    "stdout": {
      "group": "wishbone.module", 
      "module": "STDOUT", 
      "variables": {
        "purge": true
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "namedpipeserver.inbox": [
      "stdout.inbox"
    ]
  }
}