{
  "bootstrap":{
    "namedpipeserver":{
      "module":"wishbone.iomodules.namedpipeserver",
      "class":"NamedPipeServer",
      "variables":{
        "path":"/tmp/namedpipeserver"
      }
    },
    "stdout":{
      "module":"wishbone.modules.stdout",
      "class":"STDOUT",
      "variables":{
        "purge":true
      }
    }
  },
  "routingtable":{
    "namedpipeserver.inbox":[
      "stdout.inbox"
    ]
  }
}
