{
  "bootstrap":{
    "inputgenerator":{
      "module":"wishbone.iomodules.inputgenerator",
      "class":"InputGenerator",
      "variables":{
        "min_payload":0,
        "max_payload":100,
        "min_interval":0,
        "max_interval":0.01,
        "min_outage_start":5,
        "max_outage_start":10,
        "min_outage_length":10,
        "max_outage_length":20
      }
    },
    "tippingbucket":{
      "module":"wishbone.modules.tippingbucket",
      "class":"TippingBucket",
      "variables":{
        "age":10,
        "size":25000
      }
    },
    "gotmessage":{
      "module":"wishbone.modules.gotmessage",
      "class":"GotMessage",
      "variables":{
      }
    }    
  },
  "routingtable":{
    "inputgenerator.inbox":[
      "tippingbucket.inbox"
    ],
    "tippingbucket.outbox":[
      "gotmessage.inbox"
    ]
  }
}
