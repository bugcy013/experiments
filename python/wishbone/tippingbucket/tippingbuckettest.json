{
  "bootstrap": {
    "gotmessage": {
      "group": "wishbone.module", 
      "module": "GotMessage", 
      "variables": {}
    }, 
    "inputgenerator": {
      "group": "wishbone.iomodule", 
      "module": "InputGenerator", 
      "variables": {
        "max_interval": 0.01, 
        "max_outage_length": 20, 
        "max_outage_start": 10, 
        "max_payload": 100, 
        "min_interval": 0, 
        "min_outage_length": 10, 
        "min_outage_start": 5, 
        "min_payload": 0
      }
    }, 
    "tippingbucket": {
      "group": "wishbone.module", 
      "module": "TippingBucket", 
      "variables": {
        "age": 10, 
        "size": 25000
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "inputgenerator.inbox": [
      "tippingbucket.inbox"
    ], 
    "tippingbucket.outbox": [
      "gotmessage.inbox"
    ]
  }
}