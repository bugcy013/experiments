{
  "system":{
    "metrics":true,
    "metrics_interval":10,
    "metrics_dst":"logging"
  },
  "bootstrap":{
    "domainsocket":{
      "module":"wishbone.io_modules.domainsocket",
      "class":"DomainSocket",
      "variables":{
        "path":"/tmp/domainsocket"
      }
    },
    "stdout":{
      "module":"wishbone.modules.stdout",
      "class":"STDOUT",
      "variables":{
        "purge":true
      }
    }
  },
  "routingtable":{
    "domainsocket.inbox":[
      "stdout.inbox"
    ]
  }
}
