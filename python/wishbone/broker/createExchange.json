{
   "system":{
   },
   "metrics":{
      "enable":false,
      "group":"wishbone.metrics",
      "module":"Log",
      "interval":1,
      "variables":{
      }
   },
   "bootstrap":{
      "dictgen":{
         "group":"wishbone.iomodule",
         "module":"DictGenerator",
         "variables":{
             "filename":"/share/github/experiments/pyseps/FeedMongoDB/brit-a-z.txt",
             "randomize_keys":false,
             "max_elements":10,
             "sleep":0
         }
      },
      "header":{
        "group":"wishbone.module",
        "module":"BrokerHeader",
        "variables":{
            "exchange":"x",
            "key":"wishbone"
        }
      },            
      "rabbit":{
        "group":"wishbone.iomodule",
        "module":"Broker",
        "variables":{
            "host":"sandbox",
            "vhost":"/",
            "username":"guest",
            "password":"guest",
            "consume_queue":false,
            "prefetch_count":200,
            "no_ack":true,
            "delivery_mode":2
        }
      }
   },
   "routingtable":{
      "dictgen.inbox":["header.inbox"],
      "header.outbox":["rabbit.outbox"]
   }
}
