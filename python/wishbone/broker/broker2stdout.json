{
  "bootstrap": {
    "broker": {
      "group": "wishbone.iomodule", 
      "module": "Broker", 
      "variables": {
        "consume_queue": "syslog", 
        "delivery_mode": 2, 
        "host": "sandbox", 
        "no_ack": true, 
        "password": "guest", 
        "prefetch_count": 200, 
        "username": "guest", 
        "vhost": "/"
      }
    }, 
    "stdout": {
      "group": "wishbone.module", 
      "module": "STDOUT", 
      "variables": {
        "purge": true
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "broker.inbox": [
      "stdout.inbox"
    ]
  }
}