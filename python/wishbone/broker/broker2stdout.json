{
   "system":{
      "metrics":true,
      "metrics_interval":10,
      "metrics_dst":"table"
   },
   "bootstrap":{
      "broker":{
         "module":"wishbone.iomodules.broker",
         "class":"Broker",
         "variables":{
            "host":"sandbox",
            "vhost":"/",
            "username":"guest",
            "password":"guest",
            "consume_queue":"syslog",
            "prefetch_count":200,
            "no_ack":true,
            "delivery_mode":2
         }
      },
      "stdout":{
         "module":"wishbone.modules.stdout",
         "class":"STDOUT",
         "variables":{
            "purge":true
         }
      }
   },
   "routingtable":{
      "broker.inbox":[
         "stdout.inbox"
      ]
   }
}
