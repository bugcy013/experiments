{
   "system":{
      "metrics":false,
      "metrics_interval":10,
      "metrics_dst":"logging"
   },
   "bootstrap":{
      "broker":{
         "module":"wishbone.io_modules.broker",
         "class":"Broker",
         "variables":{
            "host":"sandbox",
            "vhost":"/",
            "username":"guest",
            "password":"guest",
            "consume_queue":"wishbone",
            "prefetch_count":200,
            "no_ack":true,
            "delivery_mode":2
         }
      },
      "header":{
         "module":"wishbone.modules.brokerheader",
         "class":"BrokerHeader",
         "variables":{
            "key":"wishbone",
            "exchange":""
         }
      },
      "dump":{
         "module":"wishbone.io_modules.dumpdata",
         "class":"DumpData",
         "variables":{
            "amount":5000
         }
      }
   },
   "routingtable":{
      "dump.inbox":[
         "header.inbox"
      ],
      "broker.inbox":[
         "header.inbox"
      ],
      "header.outbox":[
         "broker.outbox"
      ]
   }
}
