{
   "system":{
      "metrics":false,
      "metrics_interval":10,
      "metrics_dst":"logging"
   },
   "bootstrap":{
      "broker":{
         "module":"wishbone.io_modules.broker",
         "class":"Broker",
         "variables":{
            "host":"sandbox",
            "vhost":"/",
            "username":"guest",
            "password":"guest",
            "consume_queue":"wishbone",
            "prefetch_count":200,
            "no_ack":true,
            "delivery_mode":2
         }
      },
      "loopback":{
         "module":"wishbone.modules.brokerloopback",
         "class":"BrokerLoopback",
         "variables":{
            "key":"wishbone",
            "exchange":"",
            "dump":5000
         }
      }
   },
   "routingtable":{
      "broker.inbox":[
         "loopback.inbox"
      ],
      "loopback.outbox":[
         "broker.outbox"
      ]
   }
}
