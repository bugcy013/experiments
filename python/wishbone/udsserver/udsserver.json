{
  "bootstrap": {
    "stdout": {
      "group": "wishbone.module", 
      "module": "STDOUT", 
      "variables": {
	"purge":true
      }
    }, 
    "udsserver": {
      "group": "wishbone.iomodule", 
      "module": "UDSServer", 
      "variables": {
        "delimiter": "\n", 
        "path": "/tmp/udsserver", 
        "pool": false
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "udsserver.inbox": [
      "stdout.inbox"
    ]
  }
}
