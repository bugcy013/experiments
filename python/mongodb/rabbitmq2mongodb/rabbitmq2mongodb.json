{
  "bootstrap": {
    "broker": {
      "group": "wishbone.iomodule", 
      "module": "Broker", 
      "variables": {
        "consume_queue": "syslog", 
        "delivery_mode": 2, 
        "host": "localhost", 
        "no_ack": true, 
        "password": "guest", 
        "prefetch_count": 200, 
        "username": "guest", 
        "vhost": "/"
      }
    }, 
    "mongodb": {
      "group": "wishbone.iomodule", 
      "module": "MongoDB", 
      "variables": {
        "capped": true, 
        "collection": "input", 
        "dateconversions": [
          "@timestamp"
        ], 
        "db": "mongotails", 
        "drop_db": true, 
        "host": "localhost", 
        "max": 1000000, 
        "size": 1000000
      }
    }, 
    "validate": {
      "group": "wishbone.module", 
      "module": "JSONValidator", 
      "variables": {
        "convert": true, 
        "schema": false
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "broker.inbox": [
      "validate.inbox"
    ], 
    "validate.outbox": [
      "mongodb.outbox"
    ]
  }
}