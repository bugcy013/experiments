{
   "system":{
      "metrics":false,
      "metrics_interval":10,
      "metrics_dst":"logging"
   },
   "bootstrap":{
      "broker":{
         "module":"wishbone.iomodules.broker",
         "class":"Broker",
         "variables":{
            "host":"localhost",
            "vhost":"/",
            "username":"guest",
            "password":"guest",
            "consume_queue":"syslog",
            "prefetch_count":200,
            "no_ack":true,
            "delivery_mode":2
         }
      },
      "loadmongo":{
        "module":"loadmongo",
        "class":"LoadMongo",
        "variables":{
          "host":"localhost",
          "db":"mongotails",
          "collection":"input",
          "capped":true,
          "max":100000,
          "size":100000
        }
      }
   },
   "routingtable":{
      "broker.inbox":[
         "loadmongo.outbox"
      ]
   }
}
