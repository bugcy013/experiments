{
  "bootstrap": {
    "cursors": {
      "group": "pyseps.module", 
      "module": "TailingCursor", 
      "variables": {
        "collection": "randomdicts", 
        "db": "pyseps", 
        "host": "localhost"
      }
    }, 
    "queries": {
      "group": "wishbone.iomodule", 
      "module": "Broker", 
      "variables": {
        "auto_create": true, 
        "consume_queue": "pyseps:queries", 
        "delivery_mode": 2, 
        "host": "localhost", 
        "no_ack": false, 
        "password": "guest", 
        "prefetch_count": 1, 
        "username": "guest", 
        "vhost": "/"
      }
    }, 
    "queries_validate": {
      "group": "wishbone.module", 
      "module": "JSONValidator", 
      "variables": {
        "convert": true, 
        "schema": false
      }
    }
  }, 
  "metrics": {
    "enable": false, 
    "group": "wishbone.metrics", 
    "interval": 1, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "cursors.acknowledge": [
      "queries.acknowledge"
    ], 
    "cursors.inbox": [
      "queries.outbox"
    ], 
    "queries.inbox": [
      "queries_validate.inbox"
    ], 
    "queries_validate.outbox": [
      "cursors.queries"
    ]
  }
}