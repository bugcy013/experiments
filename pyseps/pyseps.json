{
  "system":{
  },
  "metrics":{
      "enable":false,
      "group":"wishbone.metrics",
      "module":"Log",
      "interval":1,
      "variables":{
      }
   },
  "bootstrap":{
    "queries":{
       "group":"wishbone.iomodule",
       "module":"Broker",
       "variables":{
          "host":"localhost",
          "vhost":"/",
          "username":"guest",
          "password":"guest",
          "consume_queue":"pyseps:queries",
          "prefetch_count":1,
          "no_ack":false,
          "delivery_mode":2,
          "auto_create":true
       }
    },
    "queries_validate":{
         "group":"wishbone.module",
         "module":"JSONValidator",
         "variables":{
            "schema":false,
            "convert":true
         }
    },
    "cursors":{
      "group":"pyseps.module",
      "module":"TailingCursor",
      "variables":{
        "host":"localhost",
        "db":"pyseps",
        "collection":"randomdicts"
      }
    }
  },
  "routingtable":{
    "queries.inbox":["queries_validate.inbox"],
    "queries_validate.outbox":["cursors.queries"],
    "cursors.acknowledge":["queries.acknowledge"],
    "cursors.inbox":["queries.outbox"]
  }
}
