{
  "system":{
  },
  "metrics":{
      "enable":true,
      "group":"wishbone.metrics",
      "module":"Table",
      "interval":5,
      "variables":{
      }
   },
  "bootstrap":{
    "queries":{
       "group":"wishbone.iomodule",
       "module":"Broker",
       "variables":{
          "host":"localhost",
          "vhost":"/",
          "username":"guest",
          "password":"guest",
          "consume_queue":"pyseps:queries",
          "prefetch_count":1,
          "no_ack":false,
          "delivery_mode":2,
          "auto_create":true
       }
    },
    "queries_validate":{
         "group":"wishbone.module",
         "module":"JSONValidator",
         "variables":{
            "schema":false,
            "convert":true
         }
    },
    "logs_tail":{
      "group":"pyseps.module",
      "module":"MongoTailCursor",
      "variables":{
        "host":"localhost",
        "db":"mongotails",
        "collection":"input"
      }
    }
  },
  "routingtable":{
    "queries.inbox":[
      "queries_validate.inbox"
    ],
    "queries_validate.outbox":[
      "logs_tail.queries"
    ],
    "logs_tail.acknowledge":[
      "queries.acknowledge"
    ],
    "logs_tail.inbox":[
      "queries.outbox"
    ]
  }
}
