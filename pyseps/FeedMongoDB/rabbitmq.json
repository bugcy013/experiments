{
   "system":{
   },
   "metrics":{
      "enable":true,
      "group":"wishbone.metrics",
      "module":"Log",
      "interval":5,
      "variables":{
      }
   },
   "bootstrap":{
      "logs":{
         "group":"wishbone.iomodule",
         "module":"Broker",
         "variables":{
            "host":"localhost",
            "vhost":"/",
            "username":"guest",
            "password":"guest",
            "consume_queue":"syslog",
            "prefetch_count":200,
            "no_ack":true,
            "delivery_mode":2
         }
      },
      "log_validate":{
         "group":"wishbone.module",
         "module":"JSONValidator",
         "variables":{
            "schema":false,
            "convert":true
         }
      },
      "log_mongodb":{
        "group":"wishbone.iomodule",
        "module":"MongoDB",
        "variables":{
          "host":"localhost",
          "db":"mongotails",
          "collection":"input",
          "capped":true,
          "max":1000000,
          "size":1000000,
          "drop_db":false
        }
      }
   },
   "routingtable":{
      "logs.inbox":[
        "log_validate.inbox"
      ],
      "log_validate.outbox":[
        "log_mongodb.outbox"
      ]
   }
}
