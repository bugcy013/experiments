{
   "system":{
   },
   "metrics":{
      "enable":true,
      "group":"wishbone.metrics",
      "module":"Log",
      "interval":1,
      "variables":{
      }
   },
   "bootstrap":{
      "dictgen":{
         "group":"wishbone.iomodule",
         "module":"DictGenerator",
         "variables":{
             "filename":"brit-a-z.txt"
         }
      },
    "mongodb":{
        "group":"wishbone.iomodule",
        "module":"MongoDB",
        "variables":{
          "host":"localhost",
          "db":"mongotails",
          "collection":"input",
          "capped":true,
          "max":1000000,
          "size":1000000,
          "drop_db":false
        }
      }
   },
   "routingtable":{
      "dictgen.inbox":[
         "mongodb.outbox"
      ]
   }
}
