{
   "system":{
   },
   "metrics":{
      "enable":true,
      "group":"wishbone.metrics",
      "module":"Table",
      "interval":1,
      "variables":{
      }
   },
   "bootstrap":{
      "dictgen":{
         "group":"wishbone.iomodule",
         "module":"DictGenerator",
         "variables":{
             "filename":"/share/github/experiments/pyseps/FeedMongoDB/brit-a-z.txt",
             "randomize_keys":false,
             "max_elements":10
         }
      },
      "buffer":{
        "group":"wishbone.module",
        "module":"TippingBucket",
        "variables":{
        }
      },
      "mongodb":{
        "group":"wishbone.iomodule",
        "module":"MongoDB",
        "variables":{
          "host":"localhost",
          "db":"pyseps",
          "collection":"randomdicts",
          "capped":true,
          "max":1000000,
          "size":10485760,
          "drop_db":false
        }
      }
   },
   "routingtable":{
      "dictgen.inbox":["buffer.inbox"],
      "buffer.outbox":["mongodb.outbox"]
   }
}
