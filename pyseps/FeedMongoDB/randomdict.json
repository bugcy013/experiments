{
  "bootstrap": {
    "buffer": {
      "group": "wishbone.module", 
      "module": "TippingBucket", 
      "variables": {}
    }, 
    "dictgen": {
      "group": "wishbone.iomodule", 
      "module": "DictGenerator", 
      "variables": {
        "filename": "/share/github/experiments/pyseps/FeedMongoDB/brit-a-z.txt", 
        "max_elements": 10, 
        "randomize_keys": false
      }
    }, 
    "mongodb": {
      "group": "wishbone.iomodule", 
      "module": "MongoDB", 
      "variables": {
        "capped": true, 
        "collection": "randomdicts", 
        "db": "pyseps", 
        "drop_db": false, 
        "host": "localhost", 
        "max": 1000000, 
        "size": 10485760
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 1, 
    "module": "Table", 
    "variables": {}
  }, 
  "routingtable": {
    "buffer.outbox": [
      "mongodb.outbox"
    ], 
    "dictgen.inbox": [
      "buffer.inbox"
    ]
  }
}