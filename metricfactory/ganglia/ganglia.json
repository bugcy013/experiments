{
  "system":{
    "metrics":false,
    "metrics_interval":10,
    "metrics_dst":"logging"
  },
  "bootstrap":{
    "udpserver":{
      "module":"wishbone.iomodules.udpserver",
      "class":"UDPServer",
      "variables":{
        "port":8649
      }
    },
    "ganglia":{
      "module":"metricfactory.decoders.decodeganglia",
      "class":"DecodeGanglia",
      "variables":{

      }
    },
    "buffer":{
      "module":"wishbone.modules.tippingbucket",
      "class":"TippingBucket",
      "variables":{
        "age":10,
        "size":10000
      }    
    },
    "stdout":{
      "module":"wishbone.modules.stdout",
      "class":"STDOUT",
      "variables":{
        "purge":true
      }
    }
  },
  "routingtable":{
    "udpserver.inbox":[
      "ganglia.inbox"
    ],
    "ganglia.outbox":[
      "stdout.inbox"
    ]
  }
}
