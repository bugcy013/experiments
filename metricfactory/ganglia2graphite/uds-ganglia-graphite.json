{
  "system":{
    "metrics":false,
    "metrics_interval":10,
    "metrics_dst":"logging"
  },
  "bootstrap":{
    "domainsocket":{
      "module":"wishbone.iomodules.udsserver",
      "class":"UDSServer",
      "variables":{
        "pool":true,
        "path":"/tmp/metricfactory"
      }
    },
    "ganglia":{
      "module":"metricfactory.decoders.decodeganglia",
      "class":"DecodeGanglia",
      "variables":{

      }
    },
    "graphite":{
      "module":"metricfactory.encoders.encodegraphite",
      "class":"EncodeGraphite",
      "variables":{
        "prefix":"systems"
        }
    },
    "buffer":{
      "module":"wishbone.modules.tippingbucket",
      "class":"TippingBucket",
      "variables":{
        "age":10,
        "size":10000
      }    
    },
    "tcpclient":{
      "module":"wishbone.iomodules.tcpclient",
      "class":"TCPClient",
      "variables":{
        "pool":["graphite-001:2003"]
      }
    }
  },
  "routingtable":{
    "domainsocket.inbox":[
      "ganglia.inbox"
    ],
    "ganglia.outbox":[
      "graphite.inbox"
    ],
    "graphite.outbox":[
      "buffer.inbox"
    ],
    "buffer.outbox":[
      "tcpclient.inbox"
    ]
  }
}
