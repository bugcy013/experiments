{
  "bootstrap": {
    "buffer": {
      "group": "wishbone.module", 
      "module": "TippingBucket", 
      "variables": {
        "age": 10, 
        "size": 10000
      }
    }, 
    "domainsocket": {
      "group": "wishbone.iomodule", 
      "module": "UDSServer", 
      "variables": {
        "path": "/tmp/metricfactory", 
        "pool": true
      }
    }, 
    "ganglia": {
      "group": "metricfactory.decoder", 
      "module": "DecodeGanglia", 
      "variables": {}
    }, 
    "graphite": {
      "group": "metricfactory.encoder", 
      "module": "EncodeGraphite", 
      "variables": {
        "prefix": "systems"
      }
    }, 
    "tcpclient": {
      "group": "wishbone.iomodule", 
      "module": "TCPClient", 
      "variables": {
        "pool": [
          "graphite-001:2003"
        ]
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "buffer.outbox": [
      "tcpclient.inbox"
    ], 
    "domainsocket.inbox": [
      "ganglia.inbox"
    ], 
    "ganglia.outbox": [
      "graphite.inbox"
    ], 
    "graphite.outbox": [
      "buffer.inbox"
    ]
  }
}