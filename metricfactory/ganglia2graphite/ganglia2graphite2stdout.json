{
  "system":{
    "metrics":false,
    "metrics_interval":10,
    "metrics_dst":"logging"
  },
  "bootstrap":{
    "udpserver":{
      "module":"wishbone.iomodules.udpserver",
      "class":"UDPServer",
      "variables":{
        "port":8649
      }
    },
    "ganglia":{
      "module":"metricfactory.decoders.decodeganglia",
      "class":"DecodeGanglia",
      "variables":{

      }
    },
    "graphite":{
      "module":"metricfactory.encoders.encodegraphite",
      "class":"EncodeGraphite",
      "variables":{
        "prefix":"systems"
        }
    },
    "stdout":{
      "module":"wishbone.modules.stdout",
      "class":"STDOUT",
      "variables":{
        "purge":true,
        "counter":false
      }
    }
  },
  "routingtable":{
    "udpserver.inbox":[
      "ganglia.inbox"
    ],
    "ganglia.outbox":[
      "graphite.inbox"
    ],
    "graphite.outbox":[
      "stdout.inbox"
    ]
  }
}
