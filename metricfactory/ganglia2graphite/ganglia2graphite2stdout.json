{
  "bootstrap": {
    "ganglia": {
      "group": "metricfactory.decoder", 
      "module": "DecodeGanglia", 
      "variables": {}
    }, 
    "graphite": {
      "group": "metricfactory.encoder", 
      "module": "EncodeGraphite", 
      "variables": {
        "prefix": "systems"
      }
    }, 
    "stdout": {
      "group": "wishbone.module", 
      "module": "STDOUT", 
      "variables": {
        "counter": false, 
        "purge": true
      }
    }, 
    "udpserver": {
      "group": "wishbone.iomodule", 
      "module": "UDPServer", 
      "variables": {
        "port": 8649
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "ganglia.outbox": [
      "graphite.inbox"
    ], 
    "graphite.outbox": [
      "stdout.inbox"
    ], 
    "udpserver.inbox": [
      "ganglia.inbox"
    ]
  }
}