{
  "bootstrap": {
    "domainsocketwrite": {
      "group": "wishbone.iomodule", 
      "module": "UDSClient", 
      "variables": {
        "path": "/tmp/metricfactory", 
        "pool": true
      }
    }, 
    "udpsocket": {
      "group": "wishbone.iomodule", 
      "module": "UDPServer", 
      "variables": {
        "port": 8649
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "udpsocket.inbox": [
      "domainsocketwrite.inbox"
    ]
  }
}