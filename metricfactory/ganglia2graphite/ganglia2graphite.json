{
  "system":{
    "metrics":false,
    "metrics_interval":10,
    "metrics_dst":"logging"
  },
  "bootstrap":{
    "udpsocket":{
      "module":"wishbone.io_modules.udpsocket",
      "class":"UDPSocket",
      "variables":{
        "port":8649
      }
    },
    "ganglia":{
      "module":"metricfactory.decoders.decodeganglia",
      "class":"DecodeGanglia",
      "variables":{

      }
    },
    "encodegraphite":{
        "module":"metricfactory.encoders.encodegraphite",
        "class":"EncodeGraphite",
        "variables":{
        }
    },
    "buffer":{
      "module":"wishbone.modules.tippingbucket",
      "class":"TippingBucket",
      "variables":{
        "age":10,
        "size":10000
      }
    },
    "tcpout":{
      "module":"wishbone.io_modules.tcpsocketwrite",
      "class":"TCPSocketWrite",
      "variables":{
        "pool":["yourgraphiteserver:2003"]
      }
    }
  },
  "routingtable":{
    "udpsocket.inbox":[
      "ganglia.inbox"
    ],
    "ganglia.outbox":[
      "encodegraphite.inbox"
    ],
    "encodegraphite.outbox":[
      "buffer.inbox"
    ],
    "buffer.outbox":[
      "tcpout.inbox"
    ]
  }
}
