{
  "bootstrap": {
    "buffer": {
      "group": "wishbone.module", 
      "module": "TippingBucket", 
      "variables": {
        "age": 10, 
        "size": 10000
      }
    }, 
    "encodegraphite": {
      "group": "metricfactory.encoder", 
      "module": "EncodeGraphite", 
      "variables": {}
    }, 
    "ganglia": {
      "group": "metricfactory.decoder", 
      "module": "DecodeGanglia", 
      "variables": {}
    }, 
    "tcpclient": {
      "group": "wishbone.iomodule", 
      "module": "TCPClient", 
      "variables": {
        "pool": [
          "graphite-001:2003"
        ]
      }
    }, 
    "udpserver": {
      "group": "wishbone.iomodule", 
      "module": "UDPServer", 
      "variables": {
        "port": 8649
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "buffer.outbox": [
      "tcpclient.inbox"
    ], 
    "encodegraphite.outbox": [
      "buffer.inbox"
    ], 
    "ganglia.outbox": [
      "encodegraphite.inbox"
    ], 
    "udpserver.inbox": [
      "ganglia.inbox"
    ]
  }
}