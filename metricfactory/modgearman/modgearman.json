{
  "bootstrap": {
    "modgearman": {
      "group": "wishbone.iomodule", 
      "module": "ModGearman", 
      "variables": {
        "hostnames": [
          "host1", 
          "host2", 
          "host3"
        ], 
        "secret": "xxxxxxxxxxxxxxxx", 
        "workers": 10
      }
    }, 
    "stdout": {
      "group": "wishbone.module", 
      "module": "STDOUT", 
      "variables": {
        "purge": true
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "modgearman.inbox": [
      "stdout.inbox"
    ]
  }
}