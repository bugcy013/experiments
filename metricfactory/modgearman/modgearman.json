{
  "system":{
    "metrics":false,
    "metrics_interval":10,
    "metrics_dst":"logging"
  },
  "bootstrap":{
    "modgearman":{
      "module":"wishbone.iomodules.modgearman",
      "class":"ModGearman",
      "variables":{
        "hostnames":[
          "host1",
          "host2",
          "host3"
        ],
        "secret":"xxxxxxxxxxxxxxxx",
        "workers":10
      }
    },
    "stdout":{
      "module":"wishbone.modules.stdout",
      "class":"STDOUT",
      "variables":{
        "purge":true
      }
    }
  },
  "routingtable":{
    "modgearman.inbox":[
      "stdout.inbox"
    ]
  }
}
