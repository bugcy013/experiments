{
  "bootstrap": {
    "decodemodgearman": {
      "group": "metricfactory.decoder", 
      "module": "DecodeModGearman", 
      "variables": {}
    }, 
    "graphite": {
      "group": "metricfactory.encoder", 
      "module": "EncodeGraphite", 
      "variables": {
        "prefix": "nagios"
      }
    }, 
    "modgearman": {
      "group": "wishbone.iomodule", 
      "module": "Gearmand", 
      "variables": {
        "hostnames": [
          "host1", 
          "host2", 
          "host3"
        ], 
        "secret": "xxxxxxxxxxxxxxxx", 
        "workers": 10
      }
    }, 
    "stdout": {
      "group": "wishbone.module", 
      "module": "STDOUT", 
      "variables": {
        "purge": true
      }
    }
  }, 
  "metrics": {
    "enable": true, 
    "group": "wishbone.metrics", 
    "interval": 10, 
    "module": "Log", 
    "variables": {}
  }, 
  "routingtable": {
    "decodemodgearman.outbox": [
      "graphite.inbox"
    ], 
    "graphite.outbox": [
      "stdout.inbox"
    ], 
    "modgearman.inbox": [
      "decodemodgearman.inbox"
    ]
  }
}