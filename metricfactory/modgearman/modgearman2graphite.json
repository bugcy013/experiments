{
  "system":{
    "metrics":false,
    "metrics_interval":10,
    "metrics_dst":"logging"
  },
  "bootstrap":{
    "modgearman":{
      "module":"metricfactory.iomodules.gearmand",
      "class":"Gearmand",
      "variables":{
       "hostnames":[
          "host1",
          "host2",
          "host3"
        ],
        "secret":"xxxxxxxxxxxxxxxx",
        "workers":10
      }
    },
    "decodemodgearman":{
      "module":"metricfactory.decoders.decodemodgearman",
      "class":"DecodeModGearman",
      "variables":{
      }
    },
    "graphite":{
      "module":"metricfactory.encoders.encodegraphite",
      "class":"EncodeGraphite",
      "variables":{
        "prefix":"nagios"
        }
    },
    "stdout":{
      "module":"wishbone.modules.stdout",
      "class":"STDOUT",
      "variables":{
        "purge":true
      }
    }
  },
  "routingtable":{
    "modgearman.inbox":[
      "decodemodgearman.inbox"
    ],
    "decodemodgearman.outbox":[
      "graphite.inbox"
    ], 
    "graphite.outbox":[
      "stdout.inbox"
    ]
  }
}
